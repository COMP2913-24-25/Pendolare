# EXPO_PUBLIC_PUBLISH_KEY: Obtain your publish key from your Expo account.
# More info: https://expo.dev
EXPO_PUBLIC_PUBLISH_KEY=your_expo_public_publish_key

# EXPO_PUBLIC_OSR_KEY: Retrieve this key from the Expo Router documentation/dashboard.
# More info: https://expo.github.io/router/docs
EXPO_PUBLIC_OSR_KEY=your_expo_public_osr_key

# CLERK_SECRET_KEY: Get your secret key from your Clerk dashboard.
# More info: https://clerk.dev/dashboard
CLERK_SECRET_KEY=your_clerk_secret_key

# Specify the application root folder for Expo Router.
EXPO_ROUTER_APP_ROOT=app

# Configure router import modes (sync or async).
EXPO_PUBLIC_ROUTER_IMPORT_MODE=sync
EXPO_ROUTER_IMPORT_MODE=sync

# Configure the logging level for the application.
EXPO_PUBLIC_LOG_LEVEL=error